   "source": [
    "weight = 0.5\n",
    "goal_pred = 0.8\n",
    "input = 0.5\n",
    "\n",
    "for iteration in range(20):\n",
    "    pred = input + weight\n",
    "    error = (pred - goal_pred)**2\n",
    "    direction_and_amount = (pred - goal_pred) * input\n",
    "    weight = weight - direction_and_amount\n",
    "\n",
    "print(\"Error:\" + str(error) + \"Prediction:\" + str(pred))"
   ]
